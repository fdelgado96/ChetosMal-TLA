var             ESTE
if              CAMISAKI
then            PANTALOCITO
else            QUECORTECITO
end_if          ANIANIAY
while           MIENTRAS
do              VOSSEGUIME
end_while       GRACIASPERRO
not             NIAPALOS
and             I
or              O
eq              IGUAL
not_eq          NOIGUAL
greater         REALFA
greater_eq      MASALFA
lesser          REBETA
lesser_eq       MENOSBETA
add             +
sub             -
mul             *
div             /
mod             %
assign          SELADADE
end_line        XD
par_open        (
par_close       )
print           PIPI
