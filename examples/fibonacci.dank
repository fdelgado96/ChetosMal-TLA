var n assign 15 end_line
var first assign 0 end_line
var second assign 1 end_line
var c assign 0 end_line
var next end_line

while c lesser n do
    c assign c add 1 end_line
    if c lesser_eq 1 then
        next assign c end_line
    else
        next assign first add second end_line
        first assign second end_line
        second assign next end_line
    end_if
    print next end_line
end_while